{
  "name": "tilskudd.t-fk.no",
  "description": "Løsning for søknader om tilskudd",
  "version": "2.0.27",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=4.6.2"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install && node setup.js",
    "standard-fix": "standard --fix",
    "start": "node standalone.js",
    "start-dev": "NODE_ENV=development node standalone.js"
  },
  "keywords": [
    "tilskudd",
    "søknadsskjema",
    "microsite",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/tilskudd.t-fk.no.git"
  },
  "bugs": {
    "url": "http://github.com/telemark/tilskudd.t-fk.no/issues"
  },
  "louie": {
    "versionName": "Australian Crawl",
    "versionVideoUrl": "https://www.youtube.com/watch?v=URgPL9AjkbQ",
    "systemName": "tilskudd.t-fk.no"
  },
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "devDependencies": {
    "coveralls": "2.11.16",
    "nsp": "2.6.2",
    "standard": "8.6.0",
    "tap": "10.2.1"
  },
  "dependencies": {
    "blankie": "2.0.0",
    "chairo": "3.0.0",
    "good": "7.1.0",
    "good-console": "6.4.0",
    "good-squeeze": "5.0.1",
    "handlebars": "4.0.6",
    "hapi": "16.1.0",
    "hapi-auth-cookie-issamesite-patch": "6.1.2",
    "hapi-auth-jwt2": "7.2.4",
    "inert": "4.1.0",
    "jsonwebtoken": "7.3.0",
    "scooter": "4.0.0",
    "seneca": "3.3.0",
    "seneca-balance-client": "0.6.0",
    "seneca-mesh": "0.9.0",
    "simple-encryptor": "1.1.0",
    "tfk-seneca-queue-mongodb": "1.0.8",
    "vision": "4.1.1",
    "wreck": "10.0.0",
    "yar": "8.1.2"
  }
}
